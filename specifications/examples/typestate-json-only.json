[
  {
    "style": 0,
    "name": "FileOpenClose",
    "type": "File",
    "description": "Not closing file resources can cause resource leaks.",
    "advice": "Close file resources.",
    "states": [
      {
        "name": "INIT",
        "error": false,
        "initial": true,
        "accepting": true
      },
      {
        "name": "OPENED",
        "error": true,
        "message": "file left open",
        "initial": false,
        "accepting": false
      },
      {
        "name": "CLOSED",
        "error": false,
        "initial": false,
        "accepting": true
      }
    ],
    "transitions": [
      {
        "from": "INIT",
        "method": ".*File.open.*",
        "param": 0,
        "to": "OPENED",
        "type": "OnCall"
      },
      {
        "from": "INIT",
        "method": ".*File.close.*",
        "param": 0,
        "to": "CLOSED",
        "type": "OnCall"
      },
      {
        "from": "OPENED",
        "method": ".*File.close.*",
        "param": 0,
        "to": "CLOSED",
        "type": "OnCall"
      }
    ]
  }
]
